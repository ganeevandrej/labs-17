# include <iostream>

using namespace std;

int main()
{
    int a[10];
    int n;

    cin >> n;


    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (a[i] == a[j])
                cout << i << " " << j;
        }
    }
    return 0;
}
